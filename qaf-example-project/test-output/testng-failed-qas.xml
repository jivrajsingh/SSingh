<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="QAF Example">
  <parameter name="include" value="{&#039;module&#039;:[&#039;Story&#039;]}"/>
  <parameter name="exclude" value="{}"/>
  <test name="Test Story - Test 1">
    <method-selectors>
      <method-selector>
        <script language="beanshell">
          <![CDATA[testngMethod.getMethodName().equalsIgnoreCase("Login and book a flight")]]>
        </script>
      </method-selector>
    </method-selectors>
    <parameter name="scenario.file.loc" value="scenarios/flightsBook.bdd"/>
    <classes>
      <class name="com.qmetry.qaf.automation.step.client.text.BDDTestFactory"/>
    </classes>
  </test> <!-- Test Story - Test 1 -->
</suite> <!-- QAF Example -->
